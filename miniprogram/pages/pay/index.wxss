page {
  padding-bottom: 150rpx;
  background-color: #f4f4f4;
}
.time-selector-warp {
  display: flex;
  align-items: center;
  padding-top: 24rpx;
  padding-left: 40rpx;
}
.time-selector-warp .selector-box {
  width: 220rpx;
  height: 126rpx;
  margin-right: 20rpx;
  background-color: #fff;
  border-radius: 20rpx;
  box-sizing: border-box;
  box-shadow: 0px 0px 6px 0px rgba(0, 0, 0, 0.08);
  text-align: center;
  line-height: 126rpx;
  font-size: 28rpx;
  color: #101010;
}
.time-selector-warp .time-picker {
  display: flex;
  font-weight: 500;
}
.time-selector-warp .time-picker .separator {
  width: 28rpx;
  text-align: center;
  line-height: 68rpx;
}
.time-selector-warp .time-picker .time {
  width: 68rpx;
  height: 68rpx;
  border: 2rpx solid #bbbbbb;
  border-radius: 8rpx;
  text-align: center;
  line-height: 68rpx;
}
.time-selector-warp .active {
  border: 4rpx solid #ff824e;
  color: #ff824e;
}
.order-detail-warp {
  width: 700rpx;
  margin-top: 64rpx;
  margin-left: 50%;
  transform: translateX(-50%);
  border-radius: 30rpx;
  background-color: #ffffff;
  line-height: 40rpx;
}
.order-detail-warp .food-list {
  width: 624rpx;
  margin-left: 50%;
  transform: translateX(-50%);
  padding-top: 44rpx;
  box-sizing: border-box;
  border-bottom: 3rpx solid #bbbbbb;
}
.order-detail-warp .food-list .food {
  display: flex;
  margin-bottom: 50rpx;
}
.order-detail-warp .food-list .food .food-img {
  width: 148rpx;
  height: 136rpx;
}
.order-detail-warp .food-list .food .right {
  display: flex;
  width: 76%;
}
.order-detail-warp .food-list .food .right .food-name {
  flex: 3;
  margin-left: 36rpx;
}
.order-detail-warp .food-list .food .right .price-num {
  flex: 1;
  display: flex;
  flex-direction: column;
  text-align: right;
  font-size: 22rpx;
}
.order-detail-warp .food-list .food .right .price-num .food-price-warp {
  padding-top: 10rpx;
}
.order-detail-warp .food-list .food .right .price-num .food-price-warp .price {
  font-size: 36rpx;
}
.order-detail-warp .food-list .food .right .price-num .food-num-warp {
  margin-top: 20rpx;
}
.order-detail-warp .food-list .food .right .price-num .food-num-warp .num {
  font-size: 32rpx;
}
.order-detail-warp .fee-warp {
  padding-left: 48rpx;
}
.order-detail-warp .fee-warp view {
  margin-top: 40rpx;
  font-size: 22rpx;
}
.order-detail-warp .fee-warp view .fee-name {
  margin-right: 448rpx;
  font-size: 28rpx;
}
.order-detail-warp .fee-warp view .fee {
  font-size: 36rpx;
}
.order-detail-warp .fee-warp :last-child {
  padding-bottom: 28rpx;
}
.other {
  width: 700rpx;
  height: 208rpx;
  margin-top: 30rpx;
  margin-left: 50%;
  transform: translateX(-50%);
  padding: 36rpx 48rpx 0;
  box-sizing: border-box;
  border-radius: 40rpx;
  background-color: #ffffff;
  line-height: 40rpx;
}
.other .tableware-num {
  display: flex;
  padding-bottom: 28rpx;
  border-bottom: 2rpx solid #efefef;
}
.other .notes {
  padding-top: 28rpx;
}
.other span {
  display: inline-block;
}
.other .common {
  width: 130rpx;
  font-size: 28rpx;
}
.other .tip {
  width: 216rpx;
  margin-left: 200rpx;
  margin-right: 20rpx;
  color: #c1c1c1;
  font-size: 24rpx;
  text-align: right;
  font-family: PingFangSC-regular;
}
.other .tableware-picker {
  width: 100%;
  height: 100%;
}
.other .tableware-picker .selection {
  color: #000;
}
.confirm-order-btn {
  position: fixed;
  bottom: 120rpx;
  width: 228rpx;
  height: 68rpx;
  margin-left: 50%;
  transform: translateX(-50%);
  background-color: #ff824e;
  border-radius: 20rpx;
  color: #fff;
  font-size: 32rpx;
  text-align: center;
  line-height: 68rpx;
}
