page {
  padding-bottom: 340rpx;
  background-color: #f8f8f8;
}
.tabs-warp {
  position: fixed;
  display: flex;
  width: 474rpx;
  height: 68rpx;
  margin-top: 48rpx;
  margin-left: 50%;
  transform: translateX(-50%);
  border-radius: 59rpx;
  background-color: #f7d6b1;
  color: #fff;
}
.tabs-warp .left-box {
  z-index: 3;
  width: 50%;
  height: 100%;
  padding-left: 2.5%;
}
.tabs-warp .right-box {
  z-index: 3;
  width: 50%;
  height: 100%;
  padding-right: 2.5%;
}
.tabs-warp .animation-box {
  position: absolute;
  z-index: 2;
  width: 55%;
  height: 100%;
  border-radius: 59rpx;
  background-color: #ff824e;
}
.tabs-warp .text-center {
  display: flex;
  justify-content: center;
  align-items: center;
}
.order-entry {
  position: fixed;
  top: 48rpx;
  right: 40rpx;
  display: flex;
  flex-direction: column;
  width: 44rpx;
  height: 76rpx;
}
.order-entry .icon-order {
  font-size: 44rpx;
  color: #ff824e;
}
.order-entry .order-text {
  margin-top: 4rpx;
  text-align: center;
  font-size: 19rpx;
  color: #101010;
}
.content-submit-order {
  width: 100%;
  padding-top: 166rpx;
}
.content-submit-order .target-address {
  display: flex;
  justify-content: center;
  margin-left: 50%;
  transform: translateX(-50%);
}
.content-submit-order .target-address .icon-dingwei {
  color: #f97800;
  font-size: 40rpx;
}
.content-submit-order .target-address .address-name {
  margin-top: 3rpx;
  margin-left: 13rpx;
  color: #000000;
  font-size: 28rpx;
  text-align: center;
  font-family: SourceHanSansSC-regular;
}
.content-submit-order .get-ways-warp {
  display: flex;
  justify-content: space-between;
  width: 88vw;
  margin-top: 50rpx;
  margin-left: 50%;
  transform: translateX(-50%);
  font-family: PingFangSC-regular;
}
.content-submit-order .get-ways-warp .get-way-box {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 306rpx;
  height: 306rpx;
  box-sizing: border-box;
  border-radius: 40rpx;
  background-color: #fff;
}
.content-submit-order .get-ways-warp .get-way-box .box-img {
  width: 124rpx;
  height: 124rpx;
  margin-top: 62rpx;
}
.content-submit-order .get-ways-warp .get-way-box .way-name {
  margin: 8rpx 0;
  font-size: 28rpx;
}
.content-submit-order .get-ways-warp .get-way-box .way-tip {
  font-size: 20rpx;
}
.content-submit-order .get-ways-warp .active {
  border: 4rpx solid #ff824e;
  color: #ff824e;
}
.content-submit-order .goto-submit-btn {
  width: 260rpx;
  height: 68rpx;
  margin-top: 74rpx;
  margin-left: 50%;
  transform: translateX(-50%);
  border-radius: 20rpx;
  background-color: #ff824e;
  text-align: center;
  line-height: 68rpx;
  color: #fff;
}
.content-help .address-warp {
  position: relative;
  top: 126rpx;
  left: 28rpx;
  display: flex;
  font-size: 28rpx;
  color: #c0c4cc;
}
.content-help .address-warp .address-btn-warp {
  height: 54rpx;
  color: #888888;
}
.content-help .address-warp .address-btn-warp .address-content {
  overflow: hidden;
}
.content-help .address-warp .address-btn-warp .address-content .address-btn {
  display: flex;
  background-color: #ffffff;
}
.content-help .address-warp .address-btn-warp .address-content .address-btn .icon-dian {
  display: flex;
  align-items: center;
  margin-top: 2rpx;
  font-size: 10rpx;
}
.content-help .address-warp .address-btn-warp .address-content .address-btn .se-dot {
  color: #f7d478;
}
.content-help .address-warp .address-btn-warp .address-content .address-btn .re-dot {
  color: #78b8f7;
}
.content-help .address-warp .address-btn-warp .address-content .more-address {
  padding: 10rpx;
  border-radius: 0 0 15rpx 15rpx;
  background-color: #ecebeb;
  color: #101010;
}
.content-help .address-warp .address-btn-warp .address-content .more-address .address-select {
  box-sizing: border-box;
