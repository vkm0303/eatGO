.desc {
  margin-top: 25rpx;
  width: 86%;
  margin: auto;
  border: solid 1px gainsboro;
  border-radius: 10px;
  height: 300rpx;
  padding: 15rpx;
}

.iamgeRootAll {
  margin-top: 30rpx;
  margin-bottom: 60rpx;
  display: flex;
  display: -webkit-flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
}
.click{
  width: 143px;
height: 64rpx;
border-radius: 32px;
background-color: rgba(255, 130, 78, 100);
color: rgba(255, 255, 255, 100);
font-size: 32rpx;
line-height: 64rpx;
text-align: center;
box-shadow: 0px 0px 3px 0px rgba(255, 130, 78, 100);
font-family: Microsoft Yahei;
}


.imgItem {
  margin: 6rpx;
  position: relative;
  width: 200rpx;
  height: 200rpx;
  left: 30rpx;
  margin: 5px 5px; 
}

.img {
  width: 100%;
  height: 100%;
 
}

.closeImg {
  position: absolute;
  right: 0px;
  width: 36rpx;
  height: 36rpx;
}

.photo {
  width: 170rpx;
  height: 170rpx;
  margin: 10px;
  border: solid 1px gainsboro;
  border-radius: 10px;
}
