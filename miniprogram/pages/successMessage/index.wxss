page {
  background-color: #f4f4f4;
}
.success-message {
  display: flex;
  align-items: center;
  height: 100rpx;
  margin-left: 25rpx;
  background-color: #f4f4f4;
  font-weight: 500;
  font-size: 44rpx;
}
.success-message .iconfont {
  font-size: 40rpx;
}
.outer {
  width: 85%;
  padding: 30rpx;
  margin-left: 50%;
  transform: translateX(-50%);
  background-color: #fff;
  border-radius: 20rpx;
  box-shadow: 0px 0px 6px 0px #cfcfcf;
}
.order-operation .arrival-time {
  font-size: 34rpx;
  font-weight: 600;
}
.order-operation .warp {
  display: flex;
}
.order-operation .warp .cancel-btn {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-left: 20rpx;
  font-size: 25rpx;
}
.order-operation .warp .cancel-btn .iconfont {
  font-size: 50rpx;
}
.order-operation .warp .fee-tips {
  display: flex;
  flex-direction: column;
  margin-left: 40rpx;
  padding-left: 40rpx;
  border-left: 4rpx solid #f4f4f4;
  color: #5a5959;
}
.order-operation .warp .fee-tips .title {
  font-weight: 700;
}
.order-operation .warp .fee-tips .content {
  margin-top: 10rpx;
}
.order-content {
  margin-top: 30rpx;
}
.order-content .title {
  font-size: 34rpx;
  font-weight: 700;
}
.order-content .common {
  display: flex;
  justify-content: space-between;
  margin: 20rpx 0;
  color: #9d9d9d;
}
.order-content .common .content {
  color: #000;
}
.line {
  width: 100%;
  height: 3rpx;
  margin: 30rpx 0;
  background-color: #f4f4f4;
}
