page {
  background-color: #ffffff;
}
.bgc-warp {
  position: relative;
  top: -55rpx;
  z-index: -1;
  width: 100%;
  height: 350rpx;
  box-sizing: border-box;
}
.bgc-warp .bg {
  width: 100%;
  height: 100%;
  margin-left: 4rpx;
  box-shadow: 0px 4rpx 12rpx 0px #ff9641;
}
.user-info {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  z-index: 9999;
  width: 100%;
  margin-top: -207rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #7a7a7a;
  font-family: PingFangSC-regular;
}
.user-info .user-head-pic {
  width: 240rpx;
  height: 240rpx;
  border-radius: 50%;
}
.user-info .user-name {
  margin-top: 20rpx;
}
.user-info .user-account {
  margin-top: 5rpx;
}
.login-btn {
  width: 60%;
  height: 70rpx;
  opacity: 0.9;
  margin-left: 50%;
  transform: translateX(-50%);
  background: linear-gradient(to right, #e65c00, #f8b029);
  border-radius: 15rpx;
  box-shadow: 2rpx 4rpx 8rpx #cecdcd;
  line-height: 70rpx;
  text-align: center;
  color: #ffffff;
}
.main-warp {
  position: relative;
  top: 30rpx;
  width: 100%;
}
.main-warp .common-router {
  position: relative;
  display: flex;
  align-items: center;
  width: 100%;
  height: 102rpx;
  padding-left: 64rpx;
  box-sizing: border-box;
  font-family: PingFangSC-regular;
  font-weight: 500;
  color: #7a7a7a;
}
.main-warp .common-router .icon {
  width: 40rpx;
  height: 40rpx;
}
.main-warp .common-router .common-text {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 54rpx;
  font-size: 32rpx;
}
.main-warp .common-router .icon-iconyoujiantou {
  position: absolute;
  left: 677rpx;
  font-size: 35rpx;
  font-weight: 600;
  color: #ff9f52;
}
.main-warp .common-router .btn {
  position: absolute;
  width: 686rpx;
  height: 102rpx;
  opacity: -1;
}
.zr {
  color: white;
  background: #4cb4e7;
  /*#0396FF*/
  width: 100%;
  height: 100px;
  position: relative;
}
.water {
  position: absolute;
  left: 0;
  bottom: -10px;
  height: 30px;
  width: 100%;
  z-index: 1;
}
.water-c {
  position: relative;
}
.water-1 {
  background: url("data:image/svg+xml;base64,") repeat-x;
  background-size: 600px;
  -webkit-animation: wave-animation-1 3.5s infinite linear;
  animation: wave-animation-1 3.5s infinite linear;
}
.water-2 {
  top: 5px;
  background: url("data:image/svg+xml;base64,") repeat-x;
  background-size: 600px;
  -webkit-animation: wave-animation-2 6s infinite linear;
  animation: wave-animation-2 6s infinite linear;
}
.water-1,
.water-2 {
  position: absolute;
  width: 100%;
  height: 60px;
}
.back-white {
  background: #fff;
}
@keyframes wave-animation-1 {
  0% {
    background-position: 0 top;
  }
  100% {
    background-position: 600px top;
  }
}
@keyframes wave-animation-2 {
  0% {
    background-position: 0 top;
  }
  100% {
    background-position: 600px top;
  }
}
